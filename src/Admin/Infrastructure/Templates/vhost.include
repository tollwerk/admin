ServerName {{primary_domain}}
{{#secondary_domains}}
ServerAlias {{secondary_domains}}{{/secondary_domains}}

CustomLog {{logroot}}/access_log combined

DocumentRoot "{{webroot}}"
<Directory "{{webroot}}">
	RewriteEngine on
	RewriteCond %{REQUEST_METHOD} ^(TRACE|TRACK)
	RewriteRule .* - [F]

	Options -Indexes +FollowSymLinks
	AllowOverride All
	Require all granted
</Directory>{{#php_version}}

Include {{configroot}}/fmp_{{primary_domain}}.include{{/php_version}}
